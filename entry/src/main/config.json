{
  "app": {
    "bundleName": "com.example.myapplication",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.myapplication",
    "name": ".MyApplication",
    "mainAbility": "com.example.myapplication.MainAbility",
    "deviceType": [
      "phone"
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.READ_MEDIA"
      }
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.example.myapplication.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "configChanges": [
          "orientation"
        ],
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "permissions": [
          "com.example.myapplication.DataAbilityShellProvider.PROVIDER"
        ],
        "name": "com.example.myapplication.DataAbility",
        "icon": "$media:icon",
        "description": "$string:dataability_description",
        "type": "data",
        "uri": "dataability://com.example.myapplication.DataAbility"
      },
      {
        "name": "com.example.myapplication.ServiceAbility",
        "icon": "$media:icon",
        "description": "$string:serviceability_description",
        "type": "service"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.myapplication.MainAbility2",
        "icon": "$media:icon",
        "description": "$string:mainability2_description",
        "label": "$string:entry_MainAbility2",
        "type": "page",
        "launchType": "standard"
      },
      {
        "backgroundModes": [
          "dataTransfer"
        ],
        "name": "com.example.myapplication.ServiceAbility2",
        "icon": "$media:icon",
        "description": "$string:serviceability2_description",
        "type": "service"
      }
    ]
  }
}